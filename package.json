{
  "name": "izumi",
  "version": "1.0.0",
  "type": "module",
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "del-cli ./lib",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "test": "node --experimental-strip-types --test",
    "test:setup": "cd test && npm install && node setup.js",
    "test:db": "cd test && node db-test.js",
    "test:queries": "cd test && node simple-db-test.js",
    "test:quick": "cd test && node quick-test.js",
    "test:pgvector": "cd test && node pgvector-test.js",
    "test:basic": "cd test && node basic-test.js",
    "test:interactive": "cd test && node run-queries.js",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getakita/izumi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/getakita/izumi/issues"
  },
  "homepage": "https://github.com/getakita/izumi#readme",
  "description": "A TypeScript library for converting natural language to SQL queries using LLMs, with support for DDL schemas and Drizzle ORM integration.",
  "keywords": [
    "text-to-sql",
    "sql-generation",
    "llm",
    "openai",
    "anthropic",
    "drizzle-orm",
    "ddl",
    "database",
    "typescript"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "del-cli": "^6.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "ultracite": "^5.3.1"
  },
  "dependencies": {
    "ai": "^3.3.0",
    "drizzle-orm": ">=0.30.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "drizzle-orm": ">=0.30.0"
  },
  "optionalDependencies": {
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/cohere": "^0.0.21",
    "@ai-sdk/google": "^0.0.49",
    "@ai-sdk/mistral": "^0.0.43",
    "@ai-sdk/openai": "^0.0.54",
    "dotenv": "^17.2.2",
    "pg": "^8.16.3",
    "pgvector": "^0.2.0"
  }
}
